<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/consignment_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/consignment_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyConsignmentApp"
        tools:targetApi="31">
        <activity
            android:name=".activity.SplashScreenActivity"
            android:exported="true"
            android:theme="@style/Theme.MaterialComponents.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.sharedPreference.UserPreference"
            android:exported="true"
            android:theme="@style/Theme.MyConsignmentApp.NoActionBar"/>
        <activity
            android:name=".activity.MainActivity"
            android:exported="false"
            android:label="@string/app_name"
            android:theme="@style/Theme.MyConsignmentApp.NoActionBar" />
    </application>

</manifest>